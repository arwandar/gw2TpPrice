{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "2.5.6",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x tsx src/server/main.ts",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "build": "vite build",
    "itemsList": "nodemon -w src/script/itemsList.js src/script/itemsList.js",
    "recipesList": "nodemon -w src/script/recipesList.js src/script/recipesList.js",
    "legendaries": "nodemon -w src/script/legendaries src/script/legendaries/legendaries.js",
    "guildTreks": "nodemon -w src/script/guildTreks.js src/script/guildTreks.js",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@types/react-window": "^2.0.0",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-window": "^2.2.5",
    "react-windowed-select": "^5.2.0",
    "tsx": "^4.21.0",
    "vite-express": "*"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.6",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "nodemon": "^3.1.11",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
