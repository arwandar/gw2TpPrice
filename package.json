{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "2.3.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x tsx src/server/main.ts",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "build": "vite build",
    "itemsList": "nodemon -w src/script/itemsList.js src/script/itemsList.js",
    "recipesList": "nodemon -w src/script/recipesList.js src/script/recipesList.js",
    "legendaries": "nodemon -w src/script/legendaries src/script/legendaries/legendaries.js",
    "guildTreks": "nodemon -w src/script/guildTreks.js src/script/guildTreks.js",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.1.9",
    "@types/react-window": "^1.8.8",
    "date-fns": "^4.1.0",
    "express": "^4.21.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-window": "^1.8.10",
    "react-windowed-select": "^5.2.0",
    "tsx": "^4.19.2",
    "vite-express": "*"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "nodemon": "^3.1.7",
    "typescript": "^5.7.2",
    "vite": "^6.0.2"
  }
}
